<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.lotto.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <GridPane stylesheets="@style.css">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="80.0" prefHeight="100.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button prefWidth="110" prefHeight="60" alignment="CENTER" mnemonicParsing="false"
                        GridPane.columnSpan="3" text="Sorsol" fx:id="buttonOfAll" onAction="#sorsolVagyRendezVagyTorol"/>
                <Label prefWidth="80" prefHeight="80" alignment="CENTER" id="nagy" text="90" GridPane.columnIndex="4"
                       GridPane.columnSpan="2" fx:id="nagy"/>
                <Label GridPane.rowIndex="1" fx:id="elso"/>
                <Label GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="masodik"/>
                <Label GridPane.columnIndex="2" GridPane.rowIndex="1" fx:id="harmadik"/>
                <Label GridPane.columnIndex="3" GridPane.rowIndex="1" fx:id="negyedik"/>
                <Label GridPane.columnIndex="4" GridPane.rowIndex="1" fx:id="otodik"/>
            </children>
        </GridPane>
    </children>
</VBox>
